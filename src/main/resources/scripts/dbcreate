create table "user"
(
  user_id  bigserial    not null
    constraint user_pkey
    primary key,
  name     varchar(255) not null,
  email    varchar(255),
  birthday time,
  phone    varchar(255)
);

alter table "user"
  owner to admin;

create unique index user_column_1_uindex
  on "user" (user_id);
